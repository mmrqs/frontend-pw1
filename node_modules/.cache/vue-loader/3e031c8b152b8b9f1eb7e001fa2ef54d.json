                required\n              ></v-text-field>\n\n            </v-col>\n            <v-col class=\"text-center\" cols=\"12\" sm=\"4\">\n              <div class=\"my-2\">\n                <v-btn color=\"primary\"\n                  @click=\"login\"\n                >Send</v-btn>\n              </div>\n            </v-col>\n          </v-row>\n        </v-container>\n      </v-form>\n    </v-card>\n    </v-layout>\n  </v-container>\n</template>\n\n<script>\n\nexport default {\n  name: 'Login',\n  data: () => ({\n    state: false,\n    valid: false,\n    username: '',\n    password: '',\n    todos: [],\n    mailRules: [\n      v => !!v || 'Email is required',\n      v => v.match(/^([A-Za-z0-9_\\-.])+@([A-Za-z0-9_\\-.])+\\.([A-Za-z]{2,4})$/) || 'Wrong email'\n    ],\n    passwordRules: [\n      v => !!v || 'Password is required'\n    ]\n  }),\n  methods: {\n    async login () {\n      const { username, password } = this\n      try {\n        const result = await this.axios.post('http://localhost:3000/api/v1/login', {\n          username,\n          password\n        })\n        this.$root.user = result.data\n        this.loggedIn = true\n        this.$router.push('/')\n      } catch (err) {\n        this.errorLogin = err\n      }\n    }\n\n  }\n}\n</script>\n"]}]}