 {\n        const result = await this.axios.post('http://localhost:3000/api/v1/login', {\n          username,\n          password\n        })\n        this.$root.user = result.data\n        this.loggedIn = true\n        this.$router.push('/')\n      } catch (err) {\n        this.errorLogin = err\n      }\n    }\n\n  }\n}\n</script>\n"],"sourceRoot":"src/views"}]}